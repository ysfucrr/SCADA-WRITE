{
  "name": "scada-dashboard",
  "version": "2.0.2",
  "private": true,
  "author": {
    "name": "SCADA Multicore",
    "email": "developer@example.com"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -p 3000",
    "service": "ts-node service_new.ts",
    "push": "git add . && git commit -m 'update' && git push",
    "build": "next build",
    "start-electron": "cross-env NODE_ENV=production electron .",
    "start": "yarn start-electron",
    "lint": "next lint",
    "build:service-main": "esbuild service_new.ts --bundle --platform=node --target=node18 --outfile=dist-service/service_bundle.js && node scripts/obfuscate-service.js service_bundle.js",
    "build:service-workers": "esbuild src/lib/modbus/ModbusPollerWorker.ts --bundle --platform=node --target=node18 --outfile=dist-service/ModbusPollerWorker.js && node scripts/obfuscate-service.js ModbusPollerWorker.js",
    "build:service": "yarn build:service-main && yarn build:service-workers",
    "build:electron": "tsc -p electron/tsconfig.json",
    "electron-dev": "tsc -p electron/tsconfig.json && cross-env NODE_ENV=development concurrently --kill-others \"yarn service\" \"yarn dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "yarn build:service && yarn next build && yarn build:electron",
    "electron-start": "electron electron-dist/main.js",
    "electron:package:mac": "cross-env NODE_ENV=production yarn electron-build && electron-builder build --mac",
    "electron:package:win": "cross-env NODE_ENV=production yarn electron-build && electron-builder build --win",
    "electron:package:linux": "cross-env NODE_ENV=production yarn electron-build && electron-builder build --linux",
    "electron:package": "cross-env NODE_ENV=production yarn electron-build && electron-builder build -mwl"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/express": "^4.17.21",
    "@types/pdfkit": "^0.17.0",
    "apexcharts": "^4.3.0",
    "auto-launch": "^5.0.6",
    "autoprefixer": "^10.4.20",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "child_process": "^1.0.2",
    "clsx": "^2.1.1",
    "cross-env": "^10.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "flatpickr": "^4.6.13",
    "gsap": "^3.13.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.522.0",
    "modbus-serial": "^8.0.21-no-serial-port",
    "mongodb": "^6.17.0",
    "next": "15.2.3",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "node-machine-id": "^1.1.12",
    "nodemailer": "^7.0.5",
    "p-queue": "^8.1.0",
    "pdfkit": "^0.17.1",
    "pdfkit-table": "^0.1.99",
    "react": "^19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-colorful": "^5.6.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-fitty": "^1.0.1",
    "react-icons": "^5.5.0",
    "react-number-format": "^5.4.4",
    "reactflow": "^11.11.4",
    "serialport": "^13.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sweetalert2": "^11.22.1",
    "swiper": "^11.2.0",
    "tailwind-merge": "^3.3.1",
    "tree-kill": "^1.2.2",
    "uuid": "^11.1.0",
    "wait-on": "^8.0.4",
    "xlsx": "^0.18.5"
  },
  "main": "electron-dist/main.js",
  "build": {
    "productName": "SCADA Multicore",
    "appId": "com.scada.multicore",
    "files": [
      "electron-dist/**/*",
      "electron/**/*",
      "package.json",
      "next.config.js",
      "public/**/*",
      ".next/**/*",
      "dist-service/**/*",
      "node_modules/**/*",
      ".env.local",
      "assets/**/*"
    ],
    "asarUnpack": [
      "**/*.node",
      "dist-service/**/*",
      "node_modules/serialport/**",
      "node_modules/dotenv/**",
      "node_modules/mongodb/**",
      "app/next-standalone/node_modules/**"
    ],
    "extraResources": [
      {
        "from": "assets/",
        "to": "assets/",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": ".env.local",
        "to": "app/.env.local"
      },
      {
        "from": "dist-service",
        "to": "app/dist-service"
      },
      {
        "from": ".next/standalone",
        "to": "app/next-standalone"
      },
      {
        "from": ".next/standalone/node_modules",
        "to": "app/next-standalone/node_modules"
      },
      {
        "from": ".next/static",
        "to": "app/next-standalone/.next/static"
      }
    ],
    "directories": {
      "buildResources": "resources"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icons/favicon.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "shortcutName": "SCADA Multicore",
      "createStartMenuShortcut": true,
      "runAfterFinish": false,
      "menuCategory": true
    },
    "mac": {
      "target": "dir",
      "identity": null,
      "hardenedRuntime": false
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@types/auto-launch": "^5.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-transition-group": "^4.4.12",
    "@types/uuid": "^10.0.0",
    "@types/wait-on": "^5.3.4",
    "add": "^2.0.6",
    "concurrently": "^9.2.0",
    "electron": "^37.3.0",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.25.9",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "javascript-obfuscator": "^4.1.1",
    "postcss": "^8",
    "tailwindcss": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack-obfuscator": "^3.5.1",
    "yarn": "^1.22.22"
  },
  "overrides": {
    "@react-jvectormap/core": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    },
    "@react-jvectormap/world": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    }
  }
}
